FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 73 17
HEADER "Find by Name or Property"

POPUP <FIND_NAME_TYPE>"""".

TILE
TEXT "Object type:"
TLOC 1 1
ENDTEXT

TEXT "Name filter:"
TLOC 2 5
ENDTEXT

TEXT "Value filter:"
TLOC 2 7
ENDTEXT

FIELD objtype
FLOC 14 1
ENUMSET 15
POP "FIND_NAME_TYPE"
ENDFIELD

TEXT ""
TLOC 34 1
INFO use_findfilter 38
ENDTEXT

FIELD namefilter
FLOC 16 5
STRFILLIN 13 255
ENDFIELD

FIELD valuefilter
FLOC 16 7
STRFILLIN 13 255
ENDFIELD

FIELD objlist
FLOC 2 10
LIST "" 30 6
OPTIONS alphanumsort prettyprint
ENDFIELD

FIELD destlist
FLOC 43 5
LIST "" 29 8
#OPTIONS ALPHANUMSORT
ENDFIELD

FIELD all_right
FLOC 34 13
MENUBUTTON "All->" 5 3
ENDFIELD

FIELD all_left
FLOC 34 15
MENUBUTTON "<-All" 5 3
ENDFIELD

FIELD deselect_mode
FLOC 1 22
CHECKLIST "Use 'selected objects' for a deselection operation"
ENDFIELD

FIELD done
FLOC 1 25
MENUBUTTON "OK" 10 3
ENDFIELD

FIELD cancel
FLOC 12 25
MENUBUTTON "Cancel" 10 3
ENDFIELD

FIELD select
FLOC 42 25
MENUBUTTON "Apply" 10 3
ENDFIELD

FIELD help
FLOC 63 25
MENUBUTTON "Help" 10 3
ENDFIELD

GROUP "Available objects" g1
FLOC 1 3
FSIZE 32 18
ENDGROUP

GROUP "Selected objects" g2
FLOC 42 3
FSIZE 31 18
ENDGROUP

FLEXMODE edgegravityone
FLEX g1 0 0 0.5 1
FLEX g2 0.5 0 0.5 1
FLEX namefilter 0 0 1 0
FLEX valuefilter 0 0 1 0
FLEX objlist 0 0 1 1
FLEX destlist 0 0 1 1
FLEX all_right 0.5 0 0 0
FLEX all_left 0.5 0 0 0

ENDTILE

ENDFORM
