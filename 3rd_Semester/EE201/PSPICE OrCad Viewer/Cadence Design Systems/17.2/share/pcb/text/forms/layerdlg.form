ENDFIELD

FIELD add_tolist
FLOC 29 10
MENUBUTTON "->" 7 3
ENDFIELD

FIELD del_list
FLOC 29 13
MENUBUTTON "<-" 7 3
ENDFIELD

FIELD del_all
FLOC 29 16
MENUBUTTON "<<-" 7 3
ENDFIELD

FIELD paste_up
FLOC 29 20
MENUBUTTON "Up" 7 3
ENDFIELD

FIELD paste_down
FLOC 29 23
MENUBUTTON "Down" 7 3
ENDFIELD

FIELD paste_home
FLOC 29 26
MENUBUTTON "Top" 7 3
ENDFIELD

FIELD paste_end
FLOC 29 29
