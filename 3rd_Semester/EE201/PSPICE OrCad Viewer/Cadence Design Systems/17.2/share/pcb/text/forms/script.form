FILE_TYPE=FORM_DEFN VERSION=2
FORM
FIXED
PORT 54 8
HEADER "Scripting"

MESSAGE "nofile" 3	"File does not exist: %s"
MESSAGE "needfile" 3	"Need to specify a filename"

ACTIVEFIELD "filename"

TILE

GROUP "Script File"
GLOC 1 0
GSIZE 53 9
ENDGROUP

TEXT "Name:"
TLOC 2 3
ENDTEXT

FIELD filename
FLOC 8 3
STRFILLIN 32 1023
ENDFIELD

FIELD macro
FLOC 8 6
CHECKLIST "Macro record mode"
ENDFIELD

FIELD browse
FLOC 43 2
MENUBUTTON "Browse..." 10 3
ENDFIELD

FIELD libbrowse
FLOC 43 4
MENUBUTTON "Library..." 10 3
ENDFIELD

FIELD generate
FLOC 43 6
MENUBUTTON "Generate..." 10 3
ENDFIELD

GROUP "Record/Replay"
GLOC 1 9
GSIZE 53 6
ENDGROUP

FIELD record
FLOC 2 12
MENUBUTTON "Record" 10 3
ENDFIELD

FIELD stop
FLOC 13 12
MENUBUTTON "Stop" 10 3
ENDFIELD

FIELD replay
FLOC 43 12
MENUBUTTON "Replay" 10 3
ENDFIELD

FIELD cancel
FLOC 2 16
MENUBUTTON "Cancel" 10 3
ENDFIELD

FIELD help
FLOC 43 16
MENUBUTTON "Help" 10 3
ENDFIELD

ENDTILE

ENDFORM
