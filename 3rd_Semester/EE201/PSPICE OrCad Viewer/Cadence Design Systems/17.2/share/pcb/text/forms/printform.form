FILE_TYPE=FORM_DEFN VERSION=2
FORM 
FIXED
PORT 62 6
HEADER "Scripting"

MESSAGE nofile 3 "File does not exist: %s"
MESSAGE needfile 3 "Need to specify a filename"

ACTIVEFIELD "filename"

TILE
TEXT "File:"
TLOC 2 1
ENDTEXT

FIELD filename
FLOC 8 1
STRFILLIN 32 128
ENDFIELD

FIELD close
FLOC 49 1
MENUBUTTON "Close" 10 3
ENDFIELD

FIELD buttons
FLOC 25 4
CHECKLIST "Script Buttons"
ENDFIELD

FIELD help
FLOC 49 4
MENUBUTTON "Help" 10 3
ENDFIELD

FIELD tofile
FLOC 2 6
MENUBUTTON "To File" 10 3
ENDFIELD

FIELD bprint
FLOC 13 6
MENUBUTTON "Printer" 10 3
ENDFIELD

FIELD script
FLOC 25 6
MENUBUTTON "File (Script Format)" 22 3
ENDFIELD

ENDTILE

ENDFORM
