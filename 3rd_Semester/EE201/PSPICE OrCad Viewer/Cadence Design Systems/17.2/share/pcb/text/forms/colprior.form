FILE_TYPE=FORM_DEFN VERSION=3
FORM
VARIABLE
PORT 10 37
HEADER "Priority"


TILE HEADER
TPANEL 0

FIELD on
FLOC 1 0
MENUBUTTON "On" 4 2
ENDFIELD

FIELD off
FLOC 6 0
MENUBUTTON "Off" 4 2
ENDFIELD

ENDTILE

TILE PRIORITY_TILE
TPANEL 1

FIELD COLOR_VISIBLE
FLOC 2 0
CHECKLIST ""
ENDFIELD

FIELD COLOR_BOX
FLOC 5 0
COLOR 3 1
ENDFIELD

ENDTILE

TILE StdTile
TPANEL 2
FIELD Done
FLOC 1 1
MENUBUTTON "OK" 8 2
ENDFIELD

FIELD help
FLOC 1 3
MENUBUTTON "Help" 8 2
ENDFIELD

ENDTILE

ENDFORM
